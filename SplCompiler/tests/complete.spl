ProgComplete:

DECLARATIONS

	a, b, c OF TYPE INTEGER;
	d, e, f OF TYPE CHARACTER;
	g, h, i OF TYPE REAL;

CODE

	1 -> a;
	2 -> b;
	b + a -> c;
	'a' -> d;
	'b' -> e;
	e + 1 -> f;
	a / c -> g;
	g * 2 -> h;
	2.3 -> i;

	IF a = 1 THEN
		WRITE (a);
		NEWLINE
  	ENDIF;

	IF c = (a + b) THEN
		WRITE (c, 'E', 'Q', 'U', 'A', 'L', 'S', a, 'P', 'L', 'U', 'S', b);
		NEWLINE
	ELSE
		WRITE (c, 'N', 'O', 'T', 'E', 'Q', 'U', 'A', 'L', a, 'P', 'L', 'U', 'S', b);
		NEWLINE
  	ENDIF;

	NEWLINE;
	FOR a IS 1 BY 1 TO 13 DO
		WRITE(a);
		NEWLINE
	ENDFOR;
	NEWLINE;

	1 -> a;
	WHILE NOT a <= 13 AND a > 14 DO
		WRITE(a);
		NEWLINE;
		(a + 1) -> a
	ENDWHILE;
	NEWLINE;

	1 -> a;
	DO
		WRITE(a);
		NEWLINE;
		(a + 1) -> a
	WHILE NOT a > 13 ENDDO;
	NEWLINE;

	WRITE('h','e','l','l','o', a);
	NEWLINE

ENDP ProgComplete.
