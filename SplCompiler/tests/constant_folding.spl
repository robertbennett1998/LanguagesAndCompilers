ConstantFolding:

DECLARATIONS

    int OF TYPE INTEGER;
    real OF TYPE REAL;
    char OF TYPE CHARACTER;

CODE

    2 + 2 - 4 * 2 / 2 + 2-> int;
    2 * 2 -> int;
    2 + 2 -> int;
    2 - 2 -> int;
    2 / 2 -> int;

    0.5 * 5 -> real;
    2.5 + 2.5 -> real;
    2.5 - 2.5 -> real;
    1.25 / 1.25 -> real;

    2.5 * 2 -> real;
    2.5 - 2 -> real;
    2.5 / 2 -> real;
    2.5 + 2 -> real;

    2 * 2.5 -> real;
    2 - 2.5 -> real;
    2 / 0.5 -> real;
    2 + 2.5 -> real;

    'a' + 'a' -> char;
    'c' - 'a' -> char;

ENDP ConstantFolding.
