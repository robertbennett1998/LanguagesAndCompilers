AssignmentOptimisation:

DECLARATIONS

a OF TYPE INTEGER;
b OF TYPE CHARACTER;
c OF TYPE REAL;

CODE

    1 -> a;
    2 -> a;
    WRITE(a);
    NEWLINE;

    3 -> a;
    a + 1 -> a;
    WRITE(a);
    NEWLINE;

    READ(a);
    5 -> a;
    WRITE(a);
    NEWLINE;

    6 -> a;
    READ(a);
    7 -> a;
    WRITE(a);
    NEWLINE;

    8 -> a;
    READ(a);
    a + 1 -> a;
    a + 1 -> a;
    WRITE(a);
    NEWLINE;

    'a' -> b;
    87 + a -> b;
    WRITE(b);
    NEWLINE;

    READ(b);
    'a' -> b;
    'b' -> b;
    WRITE(b);
    NEWLINE;

    READ(c);
    1.23 -> c;
    4.56 -> c;
    WRITE(c);
    NEWLINE

ENDP AssignmentOptimisation.
